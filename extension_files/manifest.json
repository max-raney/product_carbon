{
    "manifest_version": 3,
    "name": "Product Carbon",
    "description": "Base Level Extension",
    "version": "1.0",
    "permissions": ["activeTab"],
    "host_permissions": [
    "<all_urls>"
    ],
    "background": {
    "service_worker": "background.js",
    "default_icon": {
        "48": "images/logo.png"
    },
    "action": {
        "default_popup": "popup.html"
        },
    "content_scripts": [    {
        "js": ["scripts/content.js"],
        "matches": ["<all_urls>"]
    }   ]
}
